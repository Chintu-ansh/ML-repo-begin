   "source": [
    "from sklearn import tree\n",
    "\n",
    "features = [\n",
    "    [150, 0],  # Apple\n",
    "    [170, 0],  # Apple\n",
    "    [130, 1],  # Orange\n",
    "    [120, 1],  # Orange\n",
    "]\n",
    "labels = [\"apple\", \"apple\", \"orange\", \"orange\"]\n",
    "\n",
    "\n",
    "clf = tree.DecisionTreeClassifier()\n",
    "clf = clf.fit(features, labels)\n",
    "\n",
    "\n",
    "print(\"The given Fruit is \",clf.predict([[160, 0]]))  \n",
    "print(\"The given Fruit is \",clf.predict([[115, 1]]))  \n"
   ]
